{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "ChargeLogConnection": "Data Source = (localdb)\\MSSQLLocalDB; Initial Catalog = ChargeLog; Trusted_Connection=True;"
  },
  "AllowedHosts": "*",
  "InterfaceParams": {
    "MonthGroupSize": 5,
    "TimeSpanFormat": "d\\d\\ hh\\h\\ mm\\m",
    "DateFormat": "MM/dd/yyyy"
  },
  "BackendParams": {
    "ImportFolder": "D:\\ChargeLogData\\",
    "FileSearchPatten": "*.xlsx"

  }
}
